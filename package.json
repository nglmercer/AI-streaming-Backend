{
  "name": "asr_API",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "build:win-x64": "bun build --compile --target=bun-windows-x64 ./src/index.ts --outfile dist/myapp-windows-x64",
    "build:win-x64-baseline": "bun build --compile --target=bun-windows-x64-baseline ./src/index.ts --outfile dist/myapp-windows-x64-baseline",
    "build:linux-x64": "bun build --compile --target=bun-linux-x64 ./src/index.ts --outfile dist/myapp-linux-x64",
    "build:linux-arm64": "bun build --compile --target=bun-linux-arm64 ./src/index.ts --outfile dist/myapp-linux-arm64",
    "build:macos-arm64": "bun build --compile --target=bun-darwin-arm64 ./src/index.ts --outfile dist/myapp-macos-arm64",
    "build:macos-x64": "bun build --compile --target=bun-darwin-x64 ./src/index.ts --outfile dist/myapp-macos-x64"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepseek": "^0.2.16",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/mistral": "^2.0.0",
    "@ai-sdk/openai": "^1.3.23",
    "@andresaya/edge-tts": "^1.3.1",
    "@hono/node-server": "^1.17.1",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.4.0",
    "@mistralai/mistralai": "^1.7.5",
    "ai": "^4.3.19",
    "dotenv": "^17.2.1",
    "edge-tts-fork": "^1.0.0",
    "hono": "^4.8.9",
    "json-obj-manager": "^1.3.6",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/ws": "^8.18.1",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
